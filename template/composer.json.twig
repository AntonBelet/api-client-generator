{
    "name": "{{ packageName }}",
    "description": "Client library for {{ specification.getTitle() }}",
    "keywords": [
        "api-client"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">={{ phpVersion }}",
        "ext-json": "*",
        "docler-labs/api-client-base": "^1.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "{{ addslashes(namespace) }}\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{ addslashes(namespace) }}\\Test\\Unit\\": "test/suite/unit/"
        }
    }
}