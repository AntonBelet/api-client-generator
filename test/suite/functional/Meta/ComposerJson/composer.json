{
    "name": "test/petstore-api",
    "description": "Client library for Swagger Petstore",
    "keywords": [
        "api-client"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "docler-labs/api-client-base": "^3.0",
        "guzzlehttp/guzzle": "^6.5",
        "nyholm/psr7": "^1.3",
        "psr/http-client-implementation": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Test\\PetstoreApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\PetstoreApi\\Test\\Unit\\": "test/suite/unit/"
        }
    }
}
